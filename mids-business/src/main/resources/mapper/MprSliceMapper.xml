<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yinhai.mids.business.mapper.MprSliceMapper">
    <select id="queryViewTotal" resultType="java.util.Map">
        SELECT view_name, MAX(view_total)
        FROM tb_mpr_slice
        GROUP BY view_name
    </select>
</mapper>